<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >


<hibernate-mapping>
<class
    name="gov.nih.nci.iscs.oracle.pgm.hibernate.I2eActiveUserRolesVw"
    table="I2E_ACTIVE_USER_ROLES_VW"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="I2E_ACTIVE_USER_ROLES_VW"
    </meta>

        <meta attribute="class-description" inherit="false">
           @hibernate.id
            generator-class="assigned"
        </meta>
        <id
			name="npeId"
			column="NPE_ID"
			type="java.lang.Long"
			length="10"
		>
			<meta attribute="field-description">
			   @hibernate.property
				column="NPE_ID"
			</meta>
        </id>
        <property
            name="oracleId"
            column="ORACLE_ID"
            type="java.lang.String"
            length="30"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="ORACLE_ID"
            </meta>
        </property>
        <property
            name="nciLdapCn"
            column="NCI_LDAP_CN"
            type="java.lang.String"
            length="30"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="NCI_LDAP_CN"
            </meta>
        </property>
        <property
            name="npnId"
            column="NPN_ID"
            type="java.lang.Long"
            length="10"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="NPN_ID"
            </meta>
        </property>
        <property
            name="roleCode"
            column="ROLE_CODE"
            type="java.lang.String"
            length="8"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="ROLE_CODE"
            </meta>
        </property>
		<property
            name="ldapRoleName"
            column="LDAP_ROLE_NAME"
            type="java.lang.String"
            length="20"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="LDAP_ROLE_NAME"
            </meta>
        </property>
		<property
            name="ogaBranchId"
            column="OGA_BRANCH_ID"
            type="java.lang.Long"
            length="10"
        >
            <meta attribute="field-description">
               @hibernate.property
                column="OGA_BRANCH_ID"
            </meta>
        </property>
        
    <!-- associations -->

</class>
</hibernate-mapping>
