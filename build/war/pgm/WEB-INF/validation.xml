<?xml version='1.0' encoding='windows-1252'?>
<form-validation>

  <global>

        <!-- An example global constant
        <constant>
            <constant-name>fiscalYaer</constant-name>
            <constant-value>^\d{4}$</constant-value>
        </constant>
        end example-->

      <constant>
         <constant-name>zipmask</constant-name>
         <constant-value>^\d{5}(-\d{4})?$</constant-value>
      </constant>

  </global>

  <formset>
    <form name="retrieveGrantsForReferralForm">
       <field property="fyFrom" depends="mask">
          <msg name="mask" key="searchForm.fyFrom.maskmsg"/>
           <arg0 key="labels.search.field.fyFrom"/>
           <var>
               <var-name>mask</var-name>
               <var-value>^\d{4}$</var-value>
           </var>
       </field>
  
       <field property="fyTo" depends="mask">
          <msg
               name="mask"
               key="searchForm.fyTo.maskmsg"/>       
           <arg0 key="labels.search.field.fyTo"/>
           <var>
               <var-name>mask</var-name>
               <var-value>^\d{4}$</var-value>
           </var>
       </field>
    </form>
  </formset>
</form-validation>